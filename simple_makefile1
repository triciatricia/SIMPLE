# ------------------Macro-Defs---------------------
F90=ifort 
# -------------------End-macro-Defs---------------------------
# Here is the link step 
simple:simple_i_scalar.o simple_r_scalar.o simple_def_precision.o simple_d_scalar.o simple_jiffys.o simple_i_array.o simple_r_array.o simple_d_array.o simple_list_object.o simple_sll_list.o simple_syscalls.o simple_rnd.o simple_math.o simple_heapsort.o simple_aligndata.o simple_cmdline.o simple_params.o simple_spidoc.o simple_eulers.o simple_comlin.o simple_pair_wtab.o simple_stat.o simple_volspi.o simple_fftw3.o simple_ffts.o gnufor2.o simple_dens_map.o simple_imgspi.o simple_fplane.o simple_build.o simple_comlin_corr.o simple_ran_tabu.o simple_de_opt.o simple_dens_map_align.o simple_fgridvol.o simple_fvols_corr.o simple_dock.o simple_extrfplane.o simple_fplanes_corr.o simple_stkspi.o simple_fstk.o simple_mgaus.o simple_mgaus_mix.o simple_pmatch.o simple_ppca.o simple_sa_opt.o simple_tabu_end.o simple_tslist.o simple_ts_opt.o simple_rbased_search.o simple_rfree_search.o #simple.o 
	 $(F90) -o simple_i_scalar.o simple_r_scalar.o simple_def_precision.o simple_d_scalar.o simple_jiffys.o simple_i_array.o simple_r_array.o simple_d_array.o simple_list_object.o simple_sll_list.o simple_syscalls.o simple_rnd.o simple_math.o simple_heapsort.o simple_aligndata.o simple_cmdline.o simple_params.o simple_spidoc.o simple_eulers.o simple_comlin.o simple_pair_wtab.o simple_stat.o simple_volspi.o simple_fftw3.o simple_ffts.o gnufor2.o simple_dens_map.o simple_imgspi.o simple_fplane.o simple_build.o simple_comlin_corr.o simple_ran_tabu.o simple_de_opt.o simple_dens_map_align.o simple_fgridvol.o simple_fvols_corr.o simple_dock.o simple_extrfplane.o simple_fplanes_corr.o simple_stkspi.o simple_fstk.o simple_mgaus.o simple_mgaus_mix.o simple_pmatch.o simple_ppca.o simple_sa_opt.o simple_tabu_end.o simple_tslist.o simple_ts_opt.o simple_rbased_search.o simple_rfree_search.o

# Here are the compile steps

simple_i_scalar.o:./simple_i_scalar.f90  
	 $(F90) -c ./simple_i_scalar.f90 

simple_r_scalar.o:./simple_r_scalar.f90  
	 $(F90) -c ./simple_r_scalar.f90 

simple_def_precision.o:./simple_def_precision.f90  
	 $(F90) -c ./simple_def_precision.f90 

simple_d_scalar.o:./simple_d_scalar.f90 simple_def_precision.o 
	 $(F90) -c ./simple_d_scalar.f90 

simple_jiffys.o:./simple_jiffys.f90  
	 $(F90) -c ./simple_jiffys.f90 

simple_i_array.o:./simple_i_array.f90 simple_jiffys.o 
	 $(F90) -c ./simple_i_array.f90 

simple_r_array.o:./simple_r_array.f90 simple_jiffys.o 
	 $(F90) -c ./simple_r_array.f90 

simple_d_array.o:./simple_d_array.f90 simple_def_precision.o simple_jiffys.o 
	 $(F90) -c ./simple_d_array.f90 

simple_list_object.o:./simple_list_object.f90 simple_i_scalar.o simple_r_scalar.o simple_d_scalar.o simple_i_array.o simple_r_array.o simple_d_array.o 
	 $(F90) -c ./simple_list_object.f90 

simple_sll_list.o:./simple_sll_list.f90 simple_list_object.o simple_def_precision.o 
	 $(F90) -c ./simple_sll_list.f90 

simple_syscalls.o:./simple_syscalls.f90  
	 $(F90) -c ./simple_syscalls.f90 

simple_rnd.o:./simple_rnd.f90  
	 $(F90) -c ./simple_rnd.f90 

simple_math.o:./simple_math.f90 simple_def_precision.o simple_syscalls.o simple_jiffys.o simple_rnd.o 
	 $(F90) -c ./simple_math.f90 

simple_heapsort.o:./simple_heapsort.f90 simple_sll_list.o simple_math.o simple_def_precision.o 
	 $(F90) -c ./simple_heapsort.f90 

simple_aligndata.o:./simple_aligndata.f90 simple_heapsort.o simple_jiffys.o 
	 $(F90) -c ./simple_aligndata.f90 

simple_cmdline.o:./simple_cmdline.f90  
	 $(F90) -c ./simple_cmdline.f90 

simple_params.o:./simple_params.f90 simple_jiffys.o simple_cmdline.o simple_math.o 
	 $(F90) -c ./simple_params.f90 

simple_spidoc.o:./simple_spidoc.f90 simple_sll_list.o simple_jiffys.o 
	 $(F90) -c ./simple_spidoc.f90 

simple_eulers.o:./simple_eulers.f90 simple_heapsort.o simple_math.o simple_def_precision.o simple_spidoc.o simple_jiffys.o 
	 $(F90) -c ./simple_eulers.f90 

simple_comlin.o:./simple_comlin.f90 simple_eulers.o simple_math.o simple_jiffys.o simple_params.o 
	 $(F90) -c ./simple_comlin.f90 

simple_pair_wtab.o:./simple_pair_wtab.f90 simple_jiffys.o 
	 $(F90) -c ./simple_pair_wtab.f90 

simple_stat.o:./simple_stat.f90 simple_def_precision.o simple_pair_wtab.o simple_sll_list.o simple_jiffys.o simple_math.o simple_rnd.o simple_params.o 
	 $(F90) -c ./simple_stat.f90 

simple_volspi.o:./simple_volspi.f90 simple_math.o simple_jiffys.o simple_params.o simple_stat.o 
	 $(F90) -c ./simple_volspi.f90 

simple_fftw3.o:./simple_fftw3.f90  
	 $(F90) -c ./simple_fftw3.f90 

simple_ffts.o:./simple_ffts.f90 simple_fftw3.o simple_jiffys.o 
	 $(F90) -c ./simple_ffts.f90 

gnufor2.o:./gnufor2.f90  
	 $(F90) -c ./gnufor2.f90 

simple_dens_map.o:./simple_dens_map.f90 simple_math.o simple_stat.o 
	 $(F90) -c ./simple_dens_map.f90 

simple_imgspi.o:./simple_imgspi.f90 simple_math.o gnufor2.o simple_jiffys.o simple_stat.o simple_params.o simple_dens_map.o 
	 $(F90) -c ./simple_imgspi.f90 

simple_fplane.o:./simple_fplane.f90 simple_math.o simple_jiffys.o simple_params.o simple_stat.o simple_imgspi.o simple_ffts.o 
	 $(F90) -c ./simple_fplane.f90 

simple_build.o:./simple_build.f90 simple_params.o simple_eulers.o simple_comlin.o simple_heapsort.o simple_aligndata.o simple_volspi.o simple_jiffys.o simple_math.o simple_rnd.o simple_ffts.o simple_fplane.o 
	 $(F90) -c ./simple_build.f90 

simple_comlin_corr.o:./simple_comlin_corr.f90 simple_eulers.o simple_comlin.o simple_build.o 
	 $(F90) -c ./simple_comlin_corr.f90 

simple_ran_tabu.o:./simple_ran_tabu.f90 simple_rnd.o simple_jiffys.o 
	 $(F90) -c ./simple_ran_tabu.f90 

simple_de_opt.o:./simple_de_opt.f90 simple_rnd.o simple_ran_tabu.o simple_math.o simple_jiffys.o 
	 $(F90) -c ./simple_de_opt.f90 

simple_dens_map_align.o:./simple_dens_map_align.f90 simple_dens_map.o simple_de_opt.o 
	 $(F90) -c ./simple_dens_map_align.f90 

simple_fgridvol.o:./simple_fgridvol.f90 simple_eulers.o simple_params.o simple_math.o simple_build.o simple_fplane.o 
	 $(F90) -c ./simple_fgridvol.f90 

simple_fvols_corr.o:./simple_fvols_corr.f90 simple_eulers.o simple_build.o simple_params.o simple_math.o simple_jiffys.o simple_fgridvol.o 
	 $(F90) -c ./simple_fvols_corr.f90 

simple_dock.o:./simple_dock.f90 simple_eulers.o simple_jiffys.o simple_build.o simple_params.o simple_de_opt.o simple_fvols_corr.o simple_heapsort.o simple_math.o simple_volspi.o simple_syscalls.o simple_fgridvol.o 
	 $(F90) -c ./simple_dock.f90 

simple_extrfplane.o:./simple_extrfplane.f90 simple_eulers.o simple_params.o simple_math.o simple_build.o simple_def_precision.o 
	 $(F90) -c ./simple_extrfplane.f90 

simple_fplanes_corr.o:./simple_fplanes_corr.f90 simple_eulers.o simple_build.o simple_params.o simple_math.o simple_jiffys.o simple_def_precision.o 
	 $(F90) -c ./simple_fplanes_corr.f90 

simple_stkspi.o:./simple_stkspi.f90 simple_jiffys.o simple_params.o simple_imgspi.o 
	 $(F90) -c ./simple_stkspi.f90 

simple_fstk.o:./simple_fstk.f90 simple_params.o simple_fftw3.o simple_stkspi.o simple_imgspi.o simple_math.o simple_stat.o simple_build.o simple_jiffys.o simple_fplane.o 
	 $(F90) -c ./simple_fstk.f90 

simple_mgaus.o:./simple_mgaus.f90 simple_math.o simple_def_precision.o simple_jiffys.o 
	 $(F90) -c ./simple_mgaus.f90 

simple_mgaus_mix.o:./simple_mgaus_mix.f90 simple_mgaus.o simple_jiffys.o simple_rnd.o simple_params.o 
	 $(F90) -c ./simple_mgaus_mix.f90 

simple_pmatch.o:./simple_pmatch.f90 simple_fplanes_corr.o simple_math.o simple_build.o simple_params.o simple_rnd.o simple_stkspi.o simple_imgspi.o simple_jiffys.o simple_ran_tabu.o simple_fplane.o 
	 $(F90) -c ./simple_pmatch.f90 

simple_ppca.o:./simple_ppca.f90 simple_params.o simple_jiffys.o simple_rnd.o simple_math.o simple_fplane.o simple_def_precision.o 
	 $(F90) -c ./simple_ppca.f90 

simple_sa_opt.o:./simple_sa_opt.f90 simple_syscalls.o simple_rnd.o simple_ran_tabu.o simple_math.o simple_jiffys.o 
	 $(F90) -c ./simple_sa_opt.f90 

simple_tabu_end.o:./simple_tabu_end.f90 simple_jiffys.o 
	 $(F90) -c ./simple_tabu_end.f90 

simple_tslist.o:./simple_tslist.f90 simple_heapsort.o simple_math.o simple_ran_tabu.o simple_jiffys.o 
	 $(F90) -c ./simple_tslist.f90 

simple_ts_opt.o:./simple_ts_opt.f90 simple_ran_tabu.o simple_rnd.o simple_tabu_end.o simple_math.o simple_tslist.o simple_sll_list.o simple_jiffys.o 
	 $(F90) -c ./simple_ts_opt.f90 

simple_rbased_search.o:./simple_rbased_search.f90 simple_eulers.o simple_jiffys.o simple_fgridvol.o simple_rnd.o simple_spidoc.o simple_build.o simple_params.o simple_de_opt.o simple_sa_opt.o simple_aligndata.o simple_comlin_corr.o simple_fplanes_corr.o simple_ran_tabu.o simple_ts_opt.o simple_syscalls.o simple_heapsort.o simple_math.o simple_fplane.o simple_volspi.o 
	 $(F90) -c ./simple_rbased_search.f90 

simple_rfree_search.o:./simple_rfree_search.f90 simple_build.o simple_params.o simple_de_opt.o simple_sa_opt.o simple_aligndata.o simple_comlin_corr.o simple_fplanes_corr.o simple_rnd.o simple_ran_tabu.o simple_ts_opt.o simple_syscalls.o simple_math.o simple_jiffys.o 
	 $(F90) -c ./simple_rfree_search.f90 

