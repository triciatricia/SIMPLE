simple_ffts.o:../simple_ffts.f90 simple_fftw3.o simple_jiffys.o 
	 $(F90) -c ../simple_ffts.f90 -implicitnone -static -L../fftw/fftw3-3.3/ -lfftw3f -lfftw3f_threads -i-static

binarize.o:binarize.f90 simple_stkspi.o simple_imgspi.o simple_math.o simple_params.o simple_jiffys.o simple_ffts.o 
	 $(F90) -c binarize.f90 -implicitnone -static -L../fftw/fftw3-3.3/ -lfftw3f -lfftw3f_threads -i-static
# This entry allows you to type " make clean " to get rid of
# all object and module files 
clean:
	rm -f -r f_{files,modd}* *~ *.g90 *.o *.mod *.M *.d V*.inc *.vo \
	V*.f *.dbg album F.err
  
