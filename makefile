# ------------------Macro-Defs---------------------
F90=ifort 

# -------------------End-macro-Defs---------------------------

# Here is the link step 
binarize:simple_jiffys.o simple_cmdline.o simple_params.o simple_def_precision.o simple_syscalls.o simple_math.o gnufor2.o simple_pair_wtab.o simple_i_scalar.o simple_r_scalar.o simple_d_scalar.o simple_i_array.o simple_r_array.o simple_d_array.o simple_list_object.o simple_sll_list.o simple_random_nrs.o simple_stat.o dens_map_mod.o simple_imgspi.o simple_stkspi.o simple_fftw3.o simple_ffts.o binarize.o 
	 $(F90) -o binarize simple_jiffys.o simple_cmdline.o simple_params.o simple_def_precision.o simple_syscalls.o simple_math.o gnufor2.o simple_pair_wtab.o simple_i_scalar.o simple_r_scalar.o simple_d_scalar.o simple_i_array.o simple_r_array.o simple_d_array.o simple_list_object.o simple_sll_list.o simple_random_nrs.o simple_stat.o dens_map_mod.o simple_imgspi.o simple_stkspi.o simple_fftw3.o simple_ffts.o binarize.o   -implicitnone -static -L../fftw/fftw3-3.3/ -lfftw3f -lfftw3f_threads -i-static

# Here are the compile steps
 
simple_jiffys.o:../simple_jiffys.f90  
	 $(F90) -c ../simple_jiffys.f90 -implicitnone -static -L../fftw/fftw3-3.3/ -lfftw3f -lfftw3f_threads -i-static

simple_cmdline.o:../simple_cmdline.f90  
	 $(F90) -c ../simple_cmdline.f90 -implicitnone -static -L../fftw/fftw3-3.3/ -lfftw3f -lfftw3f_threads -i-static

simple_params.o:../simple_params.f90 simple_cmdline.o simple_jiffys.o 
	 $(F90) -c ../simple_params.f90 -implicitnone -static -L../fftw/fftw3-3.3/ -lfftw3f -lfftw3f_threads -i-static

simple_def_precision.o:../simple_def_precision.f90  
	 $(F90) -c ../simple_def_precision.f90 -implicitnone -static -L../fftw/fftw3-3.3/ -lfftw3f -lfftw3f_threads -i-static

simple_syscalls.o:../simple_syscalls.f90  
	 $(F90) -c ../simple_syscalls.f90 -implicitnone -static -L../fftw/fftw3-3.3/ -lfftw3f -lfftw3f_threads -i-static

simple_math.o:../simple_math.f90 simple_def_precision.o simple_syscalls.o simple_jiffys.o 
	 $(F90) -c ../simple_math.f90 -implicitnone -static -L../fftw/fftw3-3.3/ -lfftw3f -lfftw3f_threads -i-static

gnufor2.o:../gnufor2.f90  
	 $(F90) -c ../gnufor2.f90 -implicitnone -static -L../fftw/fftw3-3.3/ -lfftw3f -lfftw3f_threads -i-static

