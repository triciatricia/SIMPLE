# Here is the link step 
rotvol:simple_cmdline.o simple_jiffys.o simple_params.o simple_i_scalar.o simple_r_scalar.o simple_def_precision.o simple_d_scalar.o simple_i_array.o simple_r_array.o simple_d_array.o simple_list_object.o simple_sll_list.o simple_syscalls.o simple_rnd.o simple_math.o simple_heapsort.o simple_spidoc.o simple_eulers.o simple_comlin.o simple_aligndata.o simple_pair_wtab.o simple_stat.o simple_volspi.o simple_fftw3.o simple_ffts.o gnufor2.o simple_imgspi.o simple_fplane.o simple_build.o simple_fgridvol.o rotvol.o 
	 $(F90) -o rotvol simple_cmdline.o simple_jiffys.o simple_params.o simple_i_scalar.o simple_r_scalar.o simple_def_precision.o simple_d_scalar.o simple_i_array.o simple_r_array.o simple_d_array.o simple_list_object.o simple_sll_list.o simple_syscalls.o simple_rnd.o simple_math.o simple_heapsort.o simple_spidoc.o simple_eulers.o simple_comlin.o simple_aligndata.o simple_pair_wtab.o simple_stat.o simple_volspi.o simple_fftw3.o simple_ffts.o gnufor2.o simple_imgspi.o simple_fplane.o simple_build.o simple_fgridvol.o rotvol.o   -implicitnone -openmp -m64 -L../fftw/fftw3-osx-64/lib/ -lfftw3f -lfftw3f_threads -i-static

# Here are the compile steps
 
simple_cmdline.o:../simple_cmdline.f90  
